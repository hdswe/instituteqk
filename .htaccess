RewriteEngine On [L]


RewriteRule ^institute/([^/]*)$ index.php?institute=$1 [L]

RewriteRule ^page/([^/]*)$ page.php?seo_link=$1 [L]
RewriteRule ^blog/([^/]*)$ blog.php?cat=$1 [L]
RewriteRule ^post/([^/]*)$ post.php?seo_link=$1 [L]

#Error:
ErrorDocument 404 '<h2>Ooops 404</h2>'




RewriteRule "^norhan$"  "index.php?seo_link=norhan" 
RewriteRule ^norhan/page/([^/]*)$   page.php?seo_link=norhan&page=$1 [L]  
RewriteRule "^east$"  "index.php?seo_link=east"

RewriteRule ^east/page/([^/]*)$   page.php?seo_link=east&page=$1 [L]  
RewriteRule "^norhan$"  "index.php?seo_link=norhan" 
RewriteRule ^norhan/page/([^/]*)$   page.php?seo_link=norhan&page=$1 [L]  
